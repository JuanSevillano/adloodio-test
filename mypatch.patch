@@ -0,0 +1,21 @@
+import express, { Application } from 'express'
+import morgan from 'morgan';
+
+export class App {
+
+    private app: Application;
+
+    constructor() {
+        this.app = express();
+        this.middlewares();
+    }
+
+    middlewares() {
+        this.app.use(morgan('dev'))
+    }
+
+    listen(port: number) {
+        this.app.listen(port)
+        console.log(`listening on port ${port}`)
+    }
+}
